# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'Shipper' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Shipper
  pod 'SwiftyJSON', '~> 4.0'
  pod 'GoogleMaps'
  pod 'GooglePlaces'
  pod 'Firebase/Core'
  pod 'Firebase/Database'

  target 'ShipperTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'ShipperUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
